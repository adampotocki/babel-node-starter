{
  "name": "babel-node-starter",
  "version": "1.0.0",
  "description": "babel nodejs starter",
  "author": "Tristan Teufel",
  "homepage": "https://teufel-it.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/firsttris/babel-node-starter.git"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "nodemon": "nodemon",
    "node-dev": "node-dev src",
    "check-types": "flow src",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "babel-watch": "babel src --watch --out-dir dist",
    "babel-build": "babel src --out-dir dist",
    "webpack-build": "webpack",
    "docker-build": "docker-compose build",
    "docker-up": "docker-compose up",
    "update-dependencies": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "clean-webpack-plugin": "^0.1.18",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.43.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.65.0",
    "flow-typed": "^2.3.0",
    "jest": "^22.2.2",
    "node-dev": "3.1.3",
    "nodemon": "^1.14.12",
    "prettier": "^1.10.2",
    "webpack": "^3.11.0"
  },
  "jest": {
    "mapCoverage": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
